<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" fx:id="addItem" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.EventAddFormQuestionController">
   <children>
      <VBox fx:id="addPanel" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="600.0">
        <children>
          <Label text="Choose event type">
            <font>
              <Font size="36.0" />
            </font>
          </Label>
          <Button maxWidth="220.0" minWidth="220.0" mnemonicParsing="false" onAction="#lectureButtonPressed" prefWidth="220.0" text="Lecture">
            <font>
              <Font size="24.0" fx:id="x1" />
            </font>
            <VBox.margin>
              <Insets top="80.0" />
            </VBox.margin>
          </Button>
          <Button font="$x1" maxWidth="220.0" minWidth="220.0" mnemonicParsing="false" onAction="#workshopButtonPressed" prefWidth="220.0" text="Workshop">
            <VBox.margin>
              <Insets top="50.0" />
            </VBox.margin>
          </Button>
          <Button font="$x1" maxWidth="220.0" minWidth="220.0" mnemonicParsing="false" onAction="#seminarButtonPressed" prefWidth="220.0" text="Seminar">
            <VBox.margin>
              <Insets top="50.0" />
            </VBox.margin>
          </Button>
          <Button font="$x1" maxWidth="220.0" minWidth="220.0" mnemonicParsing="false" onAction="#tripButtonPressed" prefWidth="220.0" text="Spiritual trip">
            <VBox.margin>
              <Insets top="50.0" />
            </VBox.margin>
          </Button>
          <Button cancelButton="true" maxWidth="100.0" minWidth="100.0" mnemonicParsing="false" onAction="#cancelProcess" prefWidth="100.0" text="Cancel">
            <VBox.margin>
              <Insets top="80.0" />
            </VBox.margin>
          </Button>
        </children>
        <padding>
          <Insets top="50.0" />
        </padding>
      </VBox>
   </children>
</AnchorPane>
