<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<SplitPane dividerPositions="0.75" layoutX="0" layoutY="0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
         <items>
            <ScrollPane prefHeight="500" prefWidth="700">
               <content>
                  <TextArea fx:id="textArea" prefHeight="500" prefWidth="720" />
               </content>
            </ScrollPane>
            <GridPane maxWidth="150.0" prefHeight="640" prefWidth="0.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="150" minWidth="10.0" prefWidth="150" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="listButton" onAction="#listAll" mnemonicParsing="false" prefHeight="128.0" prefWidth="152.0" text="List all CD's" />
                  <Button fx:id="addButton" onAction="#add" mnemonicParsing="false" prefHeight="128.0" prefWidth="152.0" text="Add new CD" GridPane.rowIndex="1" />
                  <Button fx:id="removeButton" onAction="#remove" mnemonicParsing="false" prefHeight="128.0" prefWidth="152.0" text="Remove CD" GridPane.rowIndex="2" />
                  <Button fx:id="searchButton" onAction="#search" mnemonicParsing="false" prefHeight="128.0" prefWidth="152.0" text="Search CD by title" GridPane.rowIndex="3" />
                  <Button fx:id="quitButton" onAction="#quit" mnemonicParsing="false" prefHeight="128.0" prefWidth="152.0" text="Quit" GridPane.rowIndex="4" />
               </children>
            </GridPane>
         </items>
      </SplitPane>
