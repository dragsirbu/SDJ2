<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="403.0" prefWidth="757.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Manage.M_Administrators">
    <TableView fx:id="flightsTable" layoutX="225.0" prefHeight="400.0" prefWidth="532.0">
