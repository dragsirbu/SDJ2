    <TextField layoutX="14.0" layoutY="112.0" prefHeight="31.0" prefWidth="200.0" promptText="Search members" />

    <Button fx:id="confirm" layoutX="14.0" layoutY="238.0" mnemonicParsing="false" onAction="#confirmButtonPressed" prefWidth="90.0" text="Confirm" />
    <Button fx:id="forsake" layoutX="124.0" layoutY="238.0" mnemonicParsing="false" onAction="#forsakeButtonPressed" prefHeight="31.0" prefWidth="90.0" text="Forsake" />
    <Label fx:id="confirmationLabel" layoutX="68.0" layoutY="204.0" text="Are you sure?" />

</AnchorPane>
